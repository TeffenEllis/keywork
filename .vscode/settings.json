{
  "javascript.preferences.importModuleSpecifierEnding": "auto",
  "typescript.preferences.importModuleSpecifierEnding": "auto",
  "typescript.preferences.importModuleSpecifier": "shortest",
  "search.exclude": {
    "yarn.lock": true,
    "dist": true,
    "cov": true,
    "docs": true,
    "api-extractor": true,
    "site/etc": true,
    "site/temp": true,
    "site/docusaurus.config.js": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/cov": true,
    "yarn.lock": true,
    ".yarnrc": true,
    ".eslintignore": true,
    ".eslintrc.cjs": true,
    "LICENSE.md": true,
    ".editorconfig": true,
    ".node-version": true,
    ".prettierrc": true,
    ".prettierignore": true,
    "tsdoc.json": true,
    "typedoc.json": true,
    "docs": true,
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "deno.enablePaths": [
    "./lib",
    "./deno",
  ],
  "deno.unstable": false,
  "deno.lint": true,
  "deno.config": "./deno.jsonc"
}
