    "esbuild": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "undici": {
      "optional": true
    },
    "urlpattern-polyfill": {
      "optional": true
    }
  },
  "main": "./mod.js",
  "types": "./mod.d.ts",
