{
  "name": "keywork",
  "version": "7.1.0",
  "license": "AGPL-3.0",
  "packageManager": "yarn@3.2.2",
  "author": "Teffen Ellis <teffen@sister.software>",
  "description": "A batteries-included, magic-free, library for building web apps in V8 Isolates.",
  "bugs": {
    "url": "https://keywork.app/help"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "cloudflare workers",
    "deno",
    "serverless",
    "isolates",
    "react"
  ],
  "funding": [
    {
      "type": "individual",
      "url": "https://keywork.app/contributing"
    }
  ],
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">= 17.4"
  },
  "scripts": {
    "build": "node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm ./build.mts",
    "test": "deno test --coverage=./cov .",
    "test:coverage": "deno coverage --lcov ./cov"
  },
  "workspaces": [
    "site"
  ],
  "dependencies": {
    "@cloudflare/kv-asset-handler": "0.2.0",
    "classnames": "^2.3.1",
    "cookie": "^0.5.0",
    "deepmerge": "^4.3.1",
    "deepmerge-ts": "^5.1.0",
    "lodash.isplainobject": "^4.0.6",
    "negotiator": "^0.6.3",
    "ulidx": "^0.3.0",
    "urlpattern-polyfill": "^9.0.0"
  },
  "devDependencies": {
    "@deno/shim-deno": "^0.16.1",
    "@sister.software/eslint-config": "^2.0.1",
    "@sister.software/prettier-config": "^2.0.1",
    "@sister.software/typescript-esm-packager": "^1.2.10",
    "@types/cookie": "^0.5.2",
    "@types/dashify": "^1.0.1",
    "@types/lodash.isplainobject": "^4.0.7",
    "@types/negotiator": "^0.6.1",
    "@types/node": "^20.6.3",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@types/which": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "dashify": "^2.0.0",
    "esbuild": "^0.19.3",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "handlebars": "^4.7.7",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "stylelint": "^15.10.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.10",
    "typescript": "5.2.2",
    "which": "^4.0.0"
  },
  "peerDependencies": {
    "esbuild": "~0.17",
    "react": ">=18.2",
    "react-dom": ">=18.2",
    "undici": ">=5.7",
    "urlpattern-polyfill": "^5.0.5"
  },
  "peerDependenciesMeta": {
    "esbuild": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "undici": {
      "optional": true
    },
    "urlpattern-polyfill": {
      "optional": true
    }
  },
  "main": "./dist/dist/index.js",
  "types": "./dist/dist/index.d.ts",
  "exports": {
    "./package.json": "./dist/package.json",
    ".": "./dist/index.js",
    "./bundling/esbuild": "./dist/bundling/esbuild/index.js",
    "./errors": "./dist/errors/index.js",
    "./events": "./dist/events/index.js",
    "./logger": "./dist/logger/index.js",
    "./strings": "./dist/strings/index.js",
    "./timers/browser": "./dist/timers/browser/index.js",
    "./files": "./dist/files/index.js",
    "./ids": "./dist/ids/index.js",
    "./components": "./dist/components/index.js",
    "./contexts": "./dist/contexts/index.js",
    "./http": "./dist/http/index.js",
    "./http/headers": "./dist/http/headers/index.js",
    "./http/headers/caching": "./dist/http/headers/caching/index.js",
    "./http/headers/csp/directives": "./dist/http/headers/csp/directives/index.js",
    "./http/headers/csp/policies": "./dist/http/headers/csp/policies/index.js",
    "./http/headers/content-encoding": "./dist/http/headers/content-encoding/index.js",
    "./react-utils": "./dist/react/index.js",
    "./react-utils/browser": "./dist/react/browser/index.js",
    "./react-utils/node": "./dist/react/node/index.js",
    "./react-utils/worker": "./dist/react/worker/index.js",
    "./router/node": "./dist/router/node/index.js",
    "./router": "./dist/router/index.js",
    "./middleware": "./dist/middleware/index.js",
    "./uri": "./dist/uri/index.js",
    "./testing": "./dist/testing/index.js",
    "./polyfills/worker/ReadableStream": "./dist/polyfills/worker/ReadableStream/index.js",
    "./__internal/*": "./dist/__internal/*.js"
