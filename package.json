{
  "name": "@keywork/root",
  "license": "AGPL-3.0",
  "version": "1.0.0",
  "private": true,
  "description": "A library of useful tools for building web apps on Cloudflare Workers.",
  "keywords": [
    "keywork",
    "cloudflare",
    "workers",
    "cloudflare workers",
    "kv",
    "cloudflare kv",
    "key value",
    "database",
    "nosql",
    "react"
  ],
  "repository": "git@github.com:nirrius/keywork.git",
  "bugs": {
    "url": "https://github.com/nirrius/keywork/issues",
