    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "fast-glob": "^3.2.11",
    "jest": "^28.1.1",
    "miniflare": "^2.4.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  }
