{
  "name": "keywork",
  "license": "AGPL-3.0",
  "version": "3.0.1",
  "description": "A batteries-included, _magic-free_, library for building web apps on Cloudflare Workers.",
  "keywords": [
    "cloudflare",
    "workers",
    "cloudflare workers",
    "kv",
    "cloudflare kv",
    "key",
    "value",
    "key value",
    "database",
    "nosql",
    "odm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nirrius/keywork.git"
  },
  "bugs": {
    "url": "https://github.com/nirrius/keywork/issues"
  },
  "author": "Teffen Ellis <teffen@nirri.us>",
  "packageManager": "yarn@1.22.0",
  "type": "module",
  "typedocMain": "src/index.ts",
  "exports": {
    "./package.json": "./package.json"
  },
  "types": "./index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">= 17.4"
  },
  "scripts": {
    "build": "node --experimental-specifier-resolution=node ./build/build-packages.mjs",
    "build:npm": "deno run -A scripts/build-npm.ts",
    "postbuild:npm": "deno run -A scripts/format-files.ts ./dist-node/esm",
    "build:docs": "cd site && npm run build",
    "build:all": "npm run build && npm run build:docs",
    "prepublishOnly": "npm run build:all",
    "test": "jest"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "classnames": "^2.3.1",
    "cookie": "^0.5.0",
    "deepmerge": "^4.2.2",
    "http-status": "^1.5.2",
    "lodash.isplainobject": "^4.0.6",
    "ulidx": "^0.3.0"
  },
  "peerDependencies": {
    "react": ">=18.2",
    "react-dom": ">=18.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.11.0",
    "@types/cookie": "^0.5.1",
    "@types/lodash.isplainobject": "^4.0.7",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "esbuild": "^0.14.48",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "fast-glob": "^3.2.11",
    "jest": "^28.1.1",
    "miniflare": "^2.4.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "react": "^18.2",
    "react-dom": "^18.2",
    "rimraf": "^3.0.2",
    "title-case": "^3.0.3",
    "typescript": "^4.7.4"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Safari versions",
    "last 2 ChromeAndroid versions",
    "last 2 iOS versions",
    "not dead",
    "not IE 11"
  ]
}
