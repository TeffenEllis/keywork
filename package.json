  },
  "author": "Teffen Ellis <teffen@nirri.us>",
  "packageManager": "yarn@1.22.0",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">= 17.4"
  },
