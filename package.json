  "packageManager": "yarn@1.22.0",
  "type": "module",
  "engines": {
    "node": ">= 17.4"
  },
  "workspaces": [
    "./lib",
    "./site"
  ],
  "scripts": {
    "lint": "eslint ./dist",
    "build": "deno run -A scripts/build-dist.ts",
