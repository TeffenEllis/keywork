    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "jest": "^28.1.1",
    "miniflare": "^2.4.0",
    "node-stdlib-browser": "^1.2.0",
