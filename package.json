{
  "name": "keywork",
  "private": true,
  "version": "3.1.0",
  "license": "AGPL-3.0",
  "author": "Teffen Ellis <teffen@nirri.us>",
  "description": "A batteries-included, magic-free, library for building web apps in V8 Isolates.",
  "keywords": [
    "cloudflare",
    "workers",
    "cloudflare workers",
    "deno",
    "serverless",
    "isolates",
    "react"
  ],
  "packageManager": "yarn@1.22.0",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">= 17.4"
  },
  "workspaces": [
    "./lib",
    "./site"
  ],
  "scripts": {
    "lint": "eslint ./dist",
    "build": "deno run -A scripts/build-dist.ts",
    "build:docs": "cd site && npm run build",
    "build:all": "npm run build && npm run build:docs",
    "test": "jest"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "esbuild": "^0.14.48",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "fast-glob": "^3.2.11",
    "jest": "^28.1.1",
    "miniflare": "^2.4.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": ">=18.2",
    "react-dom": ">=18.2",
    "undici": ">=5.7"
  },
  "peerDependenciesMeta": {
    "react": {"optional": true},
    "react-dom": {"optional": true},
    "undici": {"optional": true}
  },
  "exports": {
    "./package.json": {
      "import": "./package.json"
    }
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/node": "^18.0.3",
    "@types/lodash.isplainobject": "^4.0.7",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "typescript": "^4.7.4"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Safari versions",
    "last 2 ChromeAndroid versions",
    "last 2 iOS versions",
    "not dead",
    "not IE 11"
  ]
