{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "sourceMap": true,
    "declarationMap": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "strict": true,
    "isolatedModules": true,
  }
}
