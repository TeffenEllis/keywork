{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "dist",
    "target": "ESNext",
    "declaration": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "paths": {
      "keywork/*": ["./*"]
    }
  },
  "exclude": [
    //
    "node_modules",
    "dist",
    "site"
  ]
}

