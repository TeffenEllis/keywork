{
	"compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
		"outDir": "dist",
    "jsx": "react-jsx",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "sourceMap": true,
    "allowJs": false,
    "declarationMap": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "strict": true,
    "isolatedModules": true,
    "paths": {
      "keywork/*": ["./*"],
    },
	},
	"exclude": [
    "node_modules",
    "dist",
    "site"
  ]
}

